{
  "name": "omdomdom",
  "version": "0.2.0",
  "description": "A virtual DOM implementation that turns strings into HTML",
  "main": "lib/omdomdom.cjs.min.js",
  "module": "lib/omdomdom.es.min.js",
  "browser": "dist/omdomdom.min.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "rollup -c rollup.config.test.js -w",
    "build": "BABEL_ENV=build rollup -c",
    "build:publish": "BABEL_ENV=publish rollup -c",
    "lint": "eslint . --ext .js -c .eslintrc.json --ignore-path .eslintignore",
    "git:update-origin": "git add . && git commit -m 'update metadata & cdn tags' && git push && git push --tags",
    "prepublishOnly": "paopu && npm run git:update-origin",
    "preversion": "npm test",
    "postversion": "npm run build:publish"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.3",
    "jest": "^26.6.3",
    "jshashes": "^1.0.8",
    "paopu": "0.0.6",
    "prettier": "^2.2.1",
    "rollup": "^2.44.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "engines": {
    "node": ">=14.7.0",
    "npm": ">=6.14.7"
  },
  "keywords": [
    "javascript",
    "js",
    "frontend",
    "dom",
    "virtual dom",
    "renderer"
  ],
  "files": [
    "lib/*.{js,map}",
    "dist/*.{js,map}",
    "src/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geotrev/omdomdom.git"
  },
  "bugs": {
    "url": "https://github.com/geotrev/omdomdom/issues"
  },
  "homepage": "https://github.com/geotrev/omdomdom",
  "author": "George Treviranus <geotrev@icloud.com>",
  "license": "MIT"
}
