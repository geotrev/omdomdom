{
  "name": "omdomdom",
  "version": "0.1.8",
  "description": "",
  "main": "lib/omdomdom.cjs.js",
  "module": "lib/omdomdom.es.js",
  "browser": "lib/omdomdom.js",
  "type": "module",
  "scripts": {
    "watch": "rollup -c rollup.config.demo.js -w",
    "build": "BABEL_ENV=build rollup -c",
    "build:publish": "BABEL_ENV=publish rollup -c",
    "lint": "eslint . --ext .js -c .eslintrc.json --ignore-path .eslintignore",
    "postversion": "npm run build:publish",
    "node:prepare": "node build/prepare-metadata.js && node build/prepare-cdn-tags.js",
    "git:update-origin": "git add . && git commit -m 'update metadata & cdn tags' && git push && git push --tags",
    "prepublishOnly": "npm run node:prepare && npm run git:update-origin"
  },
  "homepage": "https://github.com/geotrev/omdomdom",
  "author": "George Treviranus <hello@geotrev.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "jest": "^26.4.2",
    "jshashes": "^1.0.8",
    "prettier": "^2.0.5",
    "rollup": "^2.26.1",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-postcss": "^3.1.5",
    "rollup-plugin-serve": "^1.0.3",
    "rollup-plugin-terser": "^7.0.0"
  },
  "engines": {
    "node": ">=14.7.0",
    "npm": ">=6.14.7"
  },
  "keywords": [
    "javascript",
    "js",
    "frontend",
    "dom",
    "virtual dom",
    "renderer"
  ],
  "files": [
    "lib/*.{js,map}",
    "dist/*.{js,map}",
    "src/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geotrev/omdomdom.git"
  },
  "bugs": {
    "url": "https://github.com/geotrev/omdomdom/issues"
  }
